                    android:layout_width="fill_parent"
                    android:layout_height="@dimen/common_input_height"
                    aaron:fieldEnabled="false"
                    aaron:fieldName="atmcVerson"
                    aaron:fieldTitle="应用版本(ATMC)："
                    aaron:fieldType="String"
                    android:background="@color/white"
                    android:paddingLeft="@dimen/common_padding5"
                    android:paddingRight="@dimen/common_padding5" />

                <View
                    android:layout_width="fill_parent"
                    android:layout_height="@dimen/navigation_item_divide"
                    android:background="@color/navigation_item_divide" />

                <com.kingteller.client.view.KingTellerEditText
                    xmlns:aaron="http://schemas.android.com/apk/res/com.kingteller"
                    android:id="@+id/spVerson"
                    android:layout_width="fill_parent"
                    android:layout_height="@dimen/common_input_height"
                    aaron:fieldEnabled="false"
                    aaron:fieldName="spVerson"
                    aaron:fieldTitle="机芯SP版本(仅循环机)："
                    aaron:fieldType="String"
                    android:background="@color/white"
                    android:paddingLeft="@dimen/common_padding5"
                    android:paddingRight="@dimen/common_padding5" />

                <View
                    android:layout_width="fill_parent"
                    android:layout_height="@dimen/navigation_item_divide"
                    android:background="@color/navigation_item_divide" />

                <com.kingteller.client.view.KingTellerEditText
                    xmlns:aaron="http://schemas.android.com/apk/res/com.kingteller"
                    android:id="@+id/expand6"
                    android:layout_width="fill_parent"
                    android:layout_height="@dimen/common_input_height"
                    aaron:fieldEnabled="false"
                    aaron:fieldName="expand6"
                    aaron:fieldTitle="跨平台硬件驱动版本(KTWSP)："
                    aaron:fieldType="String"
                    android:background="@color/white"
                    android:paddingLeft="@dimen/common_padding5"
                    android:paddingRight="@dimen/common_padding5" />

                <View
                    android:layout_width="fill_parent"
                    android:layout_height="@dimen/navigation_item_divide"
                    android:background="@color/navigation_item_divide" />

                <com.kingteller.client.view.KingTellerEditText
                    xmlns:aaron="http://schemas.android.com/apk/res/com.kingteller"
                    android:id="@+id/expand7"
                    android:layout_width="fill_parent"
                    android:layout_height="@dimen/common_input_height"
                    aaron:fieldEnabled="false"
                    aaron:fieldName="expand7"
                    aaron:fieldTitle="BV版本(仅循环机)："
                    aaron:fieldType="String"
                    android:background="@color/white"
                    android:paddingLeft="@dimen/common_padding5"
                    android:paddingRight="@dimen/common_padding5" />

                <TextView
                    android:layout_width="fill_parent"
                    android:layout_height="@dimen/common_title_height"
                    android:layout_marginLeft="@dimen/common_margin"
                    android:gravity="left|center_vertical"
                    android:text="维护费用"
                    android:textColor="@color/black"
                    android:textSize="@dimen/common_text_title" />

                <com.kingteller.client.view.GroupListView
                    android:id="@+id/group_list"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical" >
                </com.kingteller.client.view.GroupListView>

                <TextView
                    android:layout_width="fill_parent"
                    android:layout_height="@dimen/common_title_height"
                    android:layout_marginLeft="@dimen/common_margin"
                    android:gravity="left|center_vertical"
                    android:text="附件信息"
                    android:textColor="@color/black"
                    android:textSize="@dimen/common_text_title"
                    android:visibility="gone" />

                <com.kingteller.client.view.GroupUploadFileView
                    android:id="@+id/item_add_pic"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:background="@color/white"
                    android:orientation="vertical"
                    android:visibility="gone" >
                </com.kingteller.client.view.GroupUploadFileView>
            </LinearLayout>

            <!-- 备件信息 -->

            <LinearLayout
                android:id="@+id/layout_bj"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:visibility="gone" >

                <com.kingteller.client.view.KingTellerEditText
                    xmlns:aaron="http://schemas.android.com/apk/res/com.kingteller"
                    android:id="@+id/involvesSpareParts"
                    android:layout_width="fill_parent"
                    android:layout_height="@dimen/common_input_height"
                    aaron:fieldName="involvesSpareParts"
                    aaron:fieldTitle="是否涉及备件："
                    aaron:fieldType="Select"
                    android:background="@color/white"
                    android:paddingLeft="@dimen/common_padding5"
                    android:paddingRight="@dimen/common_padding5" />

                <LinearLayout
                    android:id="@+id/layout_bj_info"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical" >

                    <TextView
                        android:layout_width="fill_parent"
                        android:layout_height="@dimen/common_title_height"
                        android:layout_marginLeft="@dimen/common_margin"
                        android:gravity="left|center_vertical"
                        android:text="维护信息"
                        android:textColor="@color/black"
                        android:textSize="@dimen/common_text_title" />

                    <com.kingteller.client.view.GroupListView
                        android:id="@+id/bj_group_list"
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical" >
                    </com.kingteller.client.view.GroupListView>
                </LinearLayout>
            </LinearLayout>
        </LinearLayout>
    </com.kingteller.client.view.KingTellerScrollView>

    <LinearLayout
        android:id="@+id/layout_btn"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:orientation="horizontal"
        android:padding="@dimen/common_padding5" >

        <Button
            android:id="@+id/btn_temp_save"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:background="@drawable/btn_orange"
            android:text="暂存"
            android:textColor="@color/white" />

        <Button
            android:id="@+id/btn_submit"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/common_margin"
            android:layout_weight="1"
            android:background="@drawable/btn_green"
            android:text="提交"
            android:textColor="@color/white" />

        <Button
            android:id="@+id/btn_sp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:background="@drawable/btn_green"
            android:text="审批"
            android:textColor="@color/white" />

        <Button
            android:id="@+id/btn_back"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/common_margin"
            android:layout_weight="1"
            android:background="@drawable/btn_red"
            android:text="退回"
            android:textColor="@color/white" />
    </LinearLayout>

    <include layout="@layout/common_loading_view" />

</RelativeLayout>