<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@color/bg"
    android:padding="@dimen/common_margin" >

    <RelativeLayout
        android:id="@+id/relativelayout"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:layout_marginLeft="@dimen/common_margin"
        android:layout_toRightOf="@+id/checkBox"
        android:orientation="horizontal" >

        <RelativeLayout
            android:id="@+id/relativelayout1"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_marginLeft="@dimen/common_margin"
            android:orientation="horizontal" >

            <TextView
                android:id="@+id/work_order_no"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/common_margin"
                android:text="工单单号:"
                android:textColor="@color/text_666666"
                android:textSize="@dimen/common_text_small" />

            <TextView
                android:id="@+id/orderNo"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/common_padding"
                android:layout_toRightOf="@+id/work_order_no"
                android:text=""
                android:textColor="@color/black"
                android:textSize="@dimen/common_text_small" />

            <TextView
                android:id="@+id/orderType"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_marginRight="@dimen/common_padding5"
                android:text=""
                android:textColor="@color/orange"
                android:textSize="@dimen/common_text_small" />
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/relativelayout2"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/relativelayout1"
            android:layout_gravity="center"
            android:layout_marginLeft="@dimen/common_margin"
            android:layout_marginTop="@dimen/common_margin"
            android:orientation="horizontal" >

            <TextView
                android:id="@+id/machineId"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/common_margin"
                android:text="机器信息:"
                android:textColor="@color/text_666666"
                android:textSize="@dimen/common_text_small" />

            <TextView
                android:id="@+id/atmMsg"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/common_padding"
                android:layout_toRightOf="@+id/machineId"
                android:text=""
                android:textColor="@color/black"
                android:textSize="@dimen/common_text_small" />
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/relativelayout3"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/relativelayout2"
            android:layout_gravity="center"
            android:layout_marginLeft="@dimen/common_margin"
            android:layout_marginTop="@dimen/common_margin"
            android:orientation="horizontal" >

            <TextView
                android:id="@+id/taskId"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/common_margin"
                android:text="任务信息:"
                android:textColor="@color/text_666666"
                android:textSize="@dimen/common_text_small" />

            <TextView
                android:id="@+id/taskInfo"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/common_padding"
                android:layout_toRightOf="@+id/taskId"
                android:text=""
                android:textColor="@color/black"
                android:textSize="@dimen/common_text_small" />
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/relativelayout4"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/relativelayout3"
            android:layout_marginLeft="@dimen/common_margin"
            android:layout_marginTop="@dimen/common_margin" >

            <TextView
                android:id="@+id/assignManText"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/common_margin"
                android:text="提交人:"
                android:textColor="@color/text_666666"
                android:textSize="@dimen/common_text_small" />

            <TextView
                android:id="@+id/assignMan"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentTop="true"
                android:layout_marginLeft="@dimen/common_padding"
                android:layout_toRightOf="@+id/assignManText"
             	android:text=""
                android:textColor="@color/black"
                android:textSize="@dimen/common_text_small" />

            <TextView
                android:id="@+id/assignTime"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_marginRight="@dimen/common_padding5"
                android:text=""
                android:textColor="@color/black"
                android:textSize="@dimen/common_text_small" />
        </RelativeLayout>
    </RelativeLayout>

    <View
        android:layout_width="fill_parent"
        android:layout_height="0.6dp"
        android:layout_below="@+id/relativelayout"
        android:layout_marginTop="@dimen/common_padding"
        android:background="@color/divider_gray" />

    <CheckBox
        android:id="@+id/checkBox"
        style="@style/common_checkbox"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_centerVertical="true"
        android:checked="false" />

</RelativeLayout>