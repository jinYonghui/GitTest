<?xml version="1.0" encoding="utf-8"?>

<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
            android:id="@+id/layout_from_other"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:layout_marginTop="@dimen/common_margin"
            android:layout_marginBottom="@dimen/common_margin">
            
           <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:background="@color/white"
                android:baselineAligned="false"
                android:orientation="horizontal"
                android:padding="@dimen/common_padding" >
                
	            <LinearLayout
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:orientation="horizontal"
                    android:paddingTop="@dimen/common_padding" >

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="托运单号："
                        android:textColor="@color/black"
                        android:textSize="@dimen/common_text_15" />

                    <TextView
                        android:id="@+id/tydh"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="@dimen/common_padding5"
                        android:text="Q/234256266"
                        android:textColor="@color/text_666666"
                        android:textSize="@dimen/common_text_14" />
                </LinearLayout>

      
            </LinearLayout>
            
              <View
                android:layout_width="fill_parent"
                android:layout_height="@dimen/navigation_item_divide"
                android:background="@color/navigation_item_divide" />
    
            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:background="@color/white"
                android:baselineAligned="false"
                android:orientation="horizontal"
                android:padding="@dimen/common_padding" >
                
	            <LinearLayout
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:orientation="horizontal"
                    android:paddingTop="@dimen/common_padding" >

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="事务类别："
                        android:textColor="@color/black"
                        android:textSize="@dimen/common_text_15" />

                    <TextView
                        android:id="@+id/swlb"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="@dimen/common_padding5"
                        android:text="加油"
                        android:textColor="@color/text_666666"
                        android:textSize="@dimen/common_text_14" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:orientation="horizontal"
                    android:paddingTop="@dimen/common_padding" >

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="到达时间:"
                        android:textColor="@color/black"
                        android:textSize="@dimen/common_text_15" />

                    <TextView
                        android:id="@+id/yqddsj"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="@dimen/common_padding5"
                        android:text="2014-10-13"
                        android:textColor="@color/text_666666"
                        android:textSize="@dimen/common_text_14" />
                </LinearLayout>
            </LinearLayout>
            
              <View
                android:layout_width="fill_parent"
                android:layout_height="@dimen/navigation_item_divide"
                android:background="@color/navigation_item_divide" />
            
              <LinearLayout 
                  android:layout_width="match_parent"
                  android:layout_height="wrap_content"
                  android:background="@color/white"
                  android:baselineAligned="false"
                  android:orientation="horizontal"
                  android:padding="@dimen/common_padding">
                
                  <LinearLayout 
                      android:layout_width="match_parent"
                      android:layout_height="wrap_content"
                      android:layout_weight="1"
                      android:orientation="horizontal">
                      
                      <TextView 
                          android:layout_width="wrap_content"
                          android:layout_height="wrap_content"
                          android:text="始发地"
                          android:textColor="@color/black"
                          android:textSize="@dimen/common_text_15"/>
                      
                      <TextView 
                          android:id="@+id/sfd"
                          android:layout_width="wrap_content"
                          android:layout_height="wrap_content"
                          android:layout_marginLeft="@dimen/common_padding5"
                          android:text="广州"
                          android:textColor="@color/text_666666"
                          android:textSize="@dimen/common_text_14"/>
                  </LinearLayout>
                  
                  <LinearLayout 
                      android:layout_width="match_parent"
                      android:layout_height="wrap_content"
                      android:layout_weight="1"
                      android:orientation="horizontal">
                      
                      <TextView 
                          android:layout_width="wrap_content"
                          android:layout_height="wrap_content"
                          android:text="目的地"
                          android:textColor="@color/black"
                          android:textSize="@dimen/common_text_15"/>
                      
                      <TextView 
                          android:id="@+id/mdd"
                          android:layout_width="wrap_content"
                          android:layout_height="wrap_content"
                          android:layout_marginLeft="@dimen/common_padding5"
                          android:text="长沙"
                          android:textColor="@color/text_666666"
                          android:textSize="@dimen/common_text_14"/>
                  </LinearLayout>
                  
              </LinearLayout>
              
              <View
                android:layout_width="fill_parent"
                android:layout_height="@dimen/navigation_item_divide"
                android:background="@color/navigation_item_divide" />
              
              <LinearLayout   
                  android:layout_width="match_parent"
                  android:layout_height="wrap_content"
                  android:background="@color/white"
                  android:baselineAligned="false"
                  android:orientation="horizontal"
                  android:padding="@dimen/common_padding">
                  
                  <TextView 
                      android:layout_width="wrap_content"
                      android:layout_height="wrap_content"
                      android:text="工作要求："
                      android:textColor="@color/black"
                      android:textSize="@dimen/common_text_15"
                      />
                  
                  <TextView 
                      android:id="@+id/swyq"
                      android:layout_width="wrap_content"
                      android:layout_height="wrap_content"
                      android:layout_marginLeft="@dimen/common_padding5"
                      android:text="完成好事情呀！！完成好事情呀！！完成好事情呀！！完成好事情呀！！完成好事情呀！！完成好事情呀！！完成好事情呀！！"
                      android:textColor="@color/text_666666"
                      android:textSize="@dimen/common_text_14"/>
                  
              </LinearLayout>
              
             <View
                android:layout_width="fill_parent"
                android:layout_height="@dimen/common_padding5"
                android:background="#99ccff" />
        </LinearLayout>
